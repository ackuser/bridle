  "immed"         : true,
  "indent"        : 2,
  "latedef"       : true,
  "newcap"        : false,
  "noarg"         : true,
  "quotmark"      : "single",
  "undef"         : true,
  "unused"        : true,
  "strict"        : false,
  "trailing"      : true,
  "forin"         : true,
  "noempty"       : true,
  "nonew"         : true,
  "funcscope"     : true,
  "maxparams"     : 4,
  "maxdepth"      : 4,
  "maxlen"        : 120,
  "globals"       : {
    "d3"     : true,
    "Bridle" : true,
    "$"      : true
  }
